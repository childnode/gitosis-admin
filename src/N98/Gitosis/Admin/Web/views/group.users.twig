{% extends "layout_page.twig" %}
{% import "_form_macros.twig" as form_macros %}

{% block content %}
    <h2>{{ 'Group'|trans }} {{ group }}: {{ 'Users'|trans }}</h2>

    <form action="#" method="post">
        {{ form_errors(form) }}
        {{ form_macros.checkbox_table(form.members) }}
        {{ form_rest(form) }}
        <input class="btn btn-primary" type="submit" name="submit" value="{{ 'save'|trans }}" />
    </form>

{% endblock %}